# Server Environment Variables (Add these to Vercel Project Settings)
NODE_ENV=production
PORT=5000

# MongoDB
MONGODB_URI=your_mongodb_connection_string

# JWT Secret
JWT_SECRET=your_jwt_secret_key

# Client URL (Your Vercel Client URL)
CLIENT_URL=https://your-client-app.vercel.app

# Firebase Service Account (JSON stringified)
FIREBASE_SERVICE_ACCOUNT={"type":"service_account",...}
